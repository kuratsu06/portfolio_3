<p style="color: green"><%= notice %></p>

<div class="top_page">
  <% @completes.each do |complete| %>
    <div class="top_img">
      <%= link_to complete_path(complete) do %>
        <% if complete.image? %>
          <%= image_tag complete.image.url, size: '300x300' %>
        <% else %>
          <p class="no_img">no image</p>
        <% end %>
      <% end %>
    </div>
  <% end %>
</div>
