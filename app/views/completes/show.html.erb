<p style="color: green"><%= notice %></p>

<%= render @complete %>

<% if @complete.liked_by?(current_user) %>
  <%= button_to complete_likes_path(@complete), method: :delete do %>
    <span style="color:red;">❤︎</span>
  <% end %>
<% else %>
  <%= button_to complete_likes_path(@complete), method: :post do %>
    <span>♡</span>
  <% end %>
<% end %>

<div>
  <%= link_to "Edit this complete", edit_complete_path(@complete) %> |
  <%= link_to "Back to completes", completes_path %>

  <%= button_to "Destroy this complete", @complete, method: :delete %>
</div>
